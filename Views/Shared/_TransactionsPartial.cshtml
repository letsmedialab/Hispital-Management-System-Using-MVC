@model string
@{
        List<KeyValuePair<string, string>> nav_controllers = new List<KeyValuePair<string, string>>()
    {
        new KeyValuePair<string, string>("Medicine Purchase","MedicinePurchase"),
        new KeyValuePair<string, string>("Doctor Bill","DoctorBill"), //2nd parameter is the controller name...
        //first one the value that will come out in the view page....
        new KeyValuePair<string, string>("Room Bill","RoomBill"),
         new KeyValuePair<string, string>("Calculate Bill","Bill")
    };
}
<div class="navbar navbar-inverse">
    <div class="navbar-header">
        <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
        </button>
    </div>
    <div class="navbar-collapse collapse">
        <ul class="nav navbar-nav">
            @foreach (KeyValuePair<string, string> nav in nav_controllers)
            {
                if (nav.Key.Equals(Model))
                {
                    <li class="active">@Html.ActionLink(nav.Key, "", nav.Value, null, new { style = "color:red;  font-size: 1.3em;line-height: 1.5; font-size: 1.3em;" })</li>
                }
                else
                {
                    <li>@Html.ActionLink(nav.Key, "", nav.Value, null, new { style = "font-size: 1.3em;line-height: 1.5; font-size: 1.3em;" })</li>
                }
            }
        </ul>
         <ul class="nav navbar-nav navbar-right">
            @{
                <li>@Html.ActionLink("Logout", "Index", "Home", null, new { style = "font-size: 1.3em;line-height: 1.5; font-size: 1.3em;color:red;" })</li>
            }
        </ul>
        <ul class="nav navbar-nav navbar-right">
            @{
                <li>@Html.ActionLink("Back", "Index", "Patient", null, new { style = "font-size: 1.3em;line-height: 1.5; font-size: 1.3em; margin-right:25px;color:red;" })</li>
            }
        </ul>

    </div>
</div>